<div class="hintergrund_cocktail_container_header">
  <%= link_to image_tag('cocktail_white', class: "home_button_single"), cocktails_path %>
  <div class="hintergrund_cocktail_container">
    <div class="container_cocktail_form">
      <h2 class="ueberschrift_new_cocktail">Add an Ingredient</h2>

      <%= render 'form', dose: @dose %>

      <%= link_to 'Back to Cocktail', cocktail_path(@cocktail), class: "ingredient_list_link" %>
    </div>
  </div>
</div>
